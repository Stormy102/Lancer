[run]
omit = 
	tests/*
	core/winutils.py
	core/spinner.py
	modules/*
	*__init__*
	conftest.py

	core/ExitCode.py
	core/ModuleExecuteState.py

	core/localisation.py
	core/CriticalProgramNotInstalled.py
	core/ModuleProvider.py
	plugins/modules/FTPAnonymousAccess.py
	plugins/modules/Gobuster.py
	plugins/modules/Nikto.py
	plugins/modules/MS08_067.py
	plugins/initmodules/Nmap.py
	plugins/modules/MS17_010.py
	plugins/modules/TelnetBanner.py
	lancer.py